{
  "name": "@atomist/cli",
  "version": "1.8.0",
  "description": "The Atomist CLI",
  "author": {
    "name": "Atomist",
    "email": "support@atomist.com",
    "url": "https://atomist.com/"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/atomist/cli.git"
  },
  "homepage": "https://github.com/atomist/cli#readme",
  "bugs": {
    "url": "https://github.com/atomist/cli/issues"
  },
  "keywords": [
    "atomist",
    "automation",
    "cli"
  ],
  "dependencies": {
    "@atomist/automation-client": "^1.6.2",
    "@atomist/sdm": "^1.6.1",
    "@atomist/sdm-core": "^1.6.1",
    "@atomist/sdm-local": "^1.1.0",
    "@atomist/sdm-pack-k8s": "^1.7.0",
    "@kubernetes/client-node": "^0.10.2",
    "@types/cross-spawn": "^6.0.0",
    "@types/express": "^4.17.0",
    "@types/fs-extra": "^8.0.0",
    "@types/git-url-parse": "^9.0.0",
    "@types/inquirer": "6.0.3",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash": "^4.14.136",
    "@types/read-pkg-up": "^3.0.1",
    "@types/tmp": "^0.1.0",
    "@types/yargs": "^13.0.0",
    "axios": "0.19.0",
    "chalk": "^2.4.2",
    "cli-highlight": "^2.1.1",
    "cli-spinner": "^0.2.10",
    "cross-spawn": "^6.0.5",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "git-url-parse": "^11.1.2",
    "inquirer": "^6.5.0",
    "js-sha256": "^0.9.0",
    "js-yaml": "^3.13.1",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.14",
    "open": "^6.4.0",
    "read-pkg-up": "^6.0.0",
    "source-map-support": "^0.5.12",
    "tmp-promise": "^2.0.2",
    "ts-essentials": "^3.0.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.2",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^9.0.2",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "rimraf": "^2.6.3",
    "supervisor": "^0.12.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "autotest": "supervisor --watch index.ts,lib,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "run-s compile test lint doc",
    "clean": "run-p clean:compile clean:doc clean:run",
    "clean:compile": "rimraf git-info.json \"index.{d.ts,js}{,.map}\" \"{lib,test}/**/*.{d.ts,js}{,.map}\" lib/typings/types.ts",
    "clean:dist": "run-s clean clean:npm",
    "clean:doc": "rimraf doc",
    "clean:npm": "rimraf node_modules",
    "clean:run": "rimraf *-v8.log profile.txt log",
    "compile": "tsc --project .",
    "doc": "typedoc --mode modules --excludeExternals --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts lib",
    "lint": "tslint --format verbose --project . --exclude \"node_modules/**\" --exclude \"**/*.d.ts\" \"**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "node bin/postInstall.js",
    "start": "node index.js",
    "test": "mocha --require espower-typescript/guess --require source-map-support/register \"test/**/*.test.ts\"",
    "test:one": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.test.ts}\"",
    "typedoc": "npm run doc"
  },
  "bin": {
    "atomist": "./index.js",
    "@atomist": "./index.js"
  },
  "engines": {
    "node": ">=8.2.0",
    "npm": ">=5.0.0"
  }
}
